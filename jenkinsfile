pipeline{
    agent any

    tools{
        maven:'maven3'
        jdk: 'jdk1.8.0_151.jdk'
    }
    stages{
    stage('Build'){
        steps {
                bat 'mvn install'
        }
        post{
            success{
                junit 'target/surefire-reports/**/*.xml'
            }
        }     
    }
    
}

}